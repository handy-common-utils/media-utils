{
  "name": "extract-audio",
  "version": "1.0.0",
  "description": "A pure-JS, no-FFmpeg audio-extractor which works with popular media formats and audio codecs",
  "scripts": {
    "pretest": "eslint . --ext .ts",
    "test": "jest",
    "compile": "shx rm -rf dist && tsc",
    "prepare": "husky",
    "prepack": "npm run compile",
    "prepublish": "npm run compile",
    "preversion": "generate-api-docs-and-update-readme && git add README.md"
  },
  "devDependencies": {
    "@handy-common-utils/dev-dependencies-jest": "^1.8.0"
  },
  "optionalDependencies": {
    "@remotion/media-parser": "^4.0.377",
    "mp4box": "^2.2.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/james-hu/extract-audio.git"
  },
  "keywords": [
    "extract",
    "audio",
    "media",
    "mp4",
    "mp3",
    "aac",
    "mov",
    "wmv",
    "stream"
  ],
  "author": "James Hu",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/james-hu/extract-audio/issues"
  },
  "homepage": "https://github.com/james-hu/extract-audio#readme"
}
